{
   "$schema":"http://json-schema.org/draft-07/schema#",
   "$id":"https://github.com/sifis-home/wp2-documents/tree/master/wp2-deliverable-2.2/schemas/app-label",
   "title":"SIFIS-Home app label schema.",
   "description":"JSON schema defining the app label structure within the SIFIS-Home framework.",
   "type":"object",
   "properties":{
      "app_name":{
         "type":"string"
      },
      "description":{
         "type":"string"
      },
      "api_labels":{
         "type":"array",
         "items":{
            "$ref":"/schemas/api-label"
         }
      }
   },
   "required":[
      "app_name",
      "description",
      "api_labels"
   ]
}
